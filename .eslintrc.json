{
  "extends": ["airbnb", "prettier", "airbnb/hooks"],
  "plugins": ["prettier", "only-warn"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": ["warn", { "singleQuote": true }],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
